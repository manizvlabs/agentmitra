 ARRAY['health insurance', 'hospitalization', 'coverage', 'benefits', 'family floater', 'senior citizen'], ARRAY[], 980, 78, 82, true, (SELECT user_id FROM lic_schema.users WHERE email = 'admin@agentmitra.com')),
