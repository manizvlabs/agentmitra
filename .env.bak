# ========================================
# Agent Mitra - Unified Environment Configuration
# ========================================
# This single .env file contains configuration for:
# - Flutter Mobile App
# - Python Backend API
# - React Config Portal
#
# DO NOT commit this file to version control
# Copy from .env.example and configure for your environment
# ========================================

# ========================================
# APPLICATION SETTINGS (Shared)
# ========================================
APP_NAME=Agent Mitra
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true

# ========================================
# FLUTTER APP CONFIGURATION
# ========================================
# API Configuration
API_BASE_URL=http://localhost:8012
WS_BASE_URL=ws://localhost:8012
API_VERSION=/api/v1
API_TIMEOUT_SECONDS=30
API_RETRY_ATTEMPTS=3
API_RETRY_DELAY_SECONDS=2

# Authentication
JWT_ACCESS_TOKEN_KEY=agent_mitra_access_token
JWT_REFRESH_TOKEN_KEY=agent_mitra_refresh_token
BIOMETRIC_ENABLED=true
AUTO_LOGIN_ENABLED=true
SESSION_TIMEOUT_MINUTES=60

# Feature Flags
ENABLE_CHATBOT=true
ENABLE_VIDEO_TUTORIALS=true
ENABLE_WHATSAPP_INTEGRATION=true
ENABLE_PAYMENT_PROCESSING=false
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_VOICE_INPUT=false
ENABLE_FILE_ATTACHMENTS=true

# External Services
WHATSAPP_BUSINESS_NUMBER=+919876543210
WHATSAPP_WEB_URL=https://wa.me/
CDN_BASE_URL=https://cdn.agentmitra.com
CDN_IMAGE_QUALITY=80
CDN_VIDEO_QUALITY=720p

# Payment Gateways
RAZORPAY_KEY_ID=your_razorpay_key_id
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# Analytics & Monitoring
ANALYTICS_ENABLED=true
CRASH_REPORTING_ENABLED=true
PERFORMANCE_MONITORING_ENABLED=true
ERROR_TRACKING_ENABLED=true

# Storage & Cache
CACHE_MAX_SIZE_MB=50
CACHE_TTL_HOURS=24
LOCAL_STORAGE_ENCRYPTION_ENABLED=true

# UI/UX Configuration
THEME_MODE=system
LANGUAGE=en
SUPPORTED_LANGUAGES=en,hi,te
DATE_FORMAT=dd/MM/yyyy
TIME_FORMAT=HH:mm
CURRENCY_SYMBOL=â‚¹
NUMBER_FORMAT=en_IN

# Notification Settings
NOTIFICATION_SOUND_ENABLED=true
NOTIFICATION_VIBRATION_ENABLED=true
NOTIFICATION_BADGE_ENABLED=true
PUSH_NOTIFICATION_ENABLED=true

# Development Settings
LOG_LEVEL=info
ENABLE_DEVELOPER_OPTIONS=false
MOCK_DATA_ENABLED=false
API_LOGGING_ENABLED=true

# Compliance & Security
DATA_ENCRYPTION_ENABLED=true
BIOMETRIC_AUTH_ENABLED=true
SCREENSHOT_PREVENTION_ENABLED=false
ROOT_DETECTION_ENABLED=true

# Performance Settings
IMAGE_COMPRESSION_QUALITY=80
VIDEO_COMPRESSION_ENABLED=true
LAZY_LOADING_ENABLED=true
PAGINATION_PAGE_SIZE=20

# Firebase Integration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_APP_ID=your_firebase_app_id
FIREBASE_MESSAGING_SENDER_ID=your_firebase_sender_id

# Pioneer Feature Flags
PIONEER_ENABLED=true
PIONEER_URL=http://localhost:4001
PIONEER_API_KEY=test-sdk-key-12345
PIONEER_SCOUT_URL=http://localhost:4002

# Platform Specific
IOS_APP_STORE_ID=com.agentmitra.app
IOS_DEEP_LINK_SCHEME=agentmitra
ANDROID_PACKAGE_NAME=com.agentmitra.app
ANDROID_DEEP_LINK_SCHEME=agentmitra
ANDROID_PLAY_STORE_URL=https://play.google.com/store/apps/details?id=com.agentmitra.app

# Third Party Integrations
GOOGLE_CLIENT_ID=your_google_client_id
FACEBOOK_APP_ID=your_facebook_app_id
INTERCOM_APP_ID=your_intercom_app_id
ZENDESK_URL=your_zendesk_url

# Experimental Features
ENABLE_AI_INSIGHTS=false
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_VOICE_COMMANDS=false
ENABLE_AR_PREVIEW=false

# ========================================
# PYTHON BACKEND CONFIGURATION
# ========================================
# Server Configuration
API_HOST=0.0.0.0
API_PORT=8012

# Database Configuration
DATABASE_URL=postgresql://agentmitra:agentmitra_dev@localhost:5432/agentmitra_dev
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600
DB_ECHO=false

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_SOCKET_TIMEOUT=5
REDIS_SOCKET_CONNECT_TIMEOUT=5

# Security & Authentication
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production-minimum-32-chars
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=15
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# OTP Configuration
OTP_EXPIRY_MINUTES=5
OTP_MAX_ATTEMPTS=3
OTP_RATE_LIMIT_PER_HOUR=5

# Rate Limiting
RATE_LIMITING_ENABLED=true
RATE_LIMIT_DEFAULT=100/minute
RATE_LIMIT_AUTH=10000/hour
RATE_LIMIT_OTP=50/hour
RATE_LIMIT_IMPORT=50/minute
RATE_LIMIT_ANALYTICS=500/minute

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.7
OPENAI_ORGANIZATION=your-org-id

# SMS Provider Configuration
SMS_PROVIDER=twilio
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_MESSAGING_SERVICE_SID=your-messaging-service-sid
SMS_API_KEY=your-sms-api-key
SMS_API_SECRET=your-sms-api-secret
SMS_FROM_NUMBER=+1234567890
SMS_SENDER_ID=AGMITR

# AWS Configuration (for SNS)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Email Provider Configuration
EMAIL_PROVIDER=smtp
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_FROM=your-email@gmail.com

# FeatureHub Configuration
FEATUREHUB_URL=http://localhost:8071
FEATUREHUB_API_KEY=your-featurehub-api-key
FEATUREHUB_ENVIRONMENT=development
FEATUREHUB_SDK_KEY=your-featurehub-sdk-key
FEATUREHUB_POLL_INTERVAL=30
FEATUREHUB_STREAMING=true
FEATUREHUB_ADMIN_TOKEN=your-admin-token
FEATUREHUB_ADMIN_SDK_URL=http://localhost:8071

# MinIO Storage Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=agentmitra-media
MINIO_USE_SSL=false
MINIO_CDN_BASE_URL=http://localhost:9000/agentmitra-media

# WhatsApp Business API Configuration
WHATSAPP_API_URL=https://graph.facebook.com/v18.0
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id
WHATSAPP_BUSINESS_NUMBER=your-whatsapp-business-number
WHATSAPP_WEBHOOK_SECRET=your-webhook-secret
WHATSAPP_VERIFY_TOKEN=your-verify-token
WHATSAPP_WEBHOOK_URL=https://your-domain.com/api/v1/whatsapp/webhook

# YouTube Integration
YOUTUBE_API_KEY=your-youtube-api-key
YOUTUBE_CHANNEL_ID=your-youtube-channel-id
YOUTUBE_CLIENT_ID=your-youtube-oauth-client-id
YOUTUBE_CLIENT_SECRET=your-youtube-oauth-client-secret
YOUTUBE_UPLOAD_ENABLED=true
YOUTUBE_DEFAULT_PRIVACY=unlisted

# Video Tutorial System
VIDEO_MAX_FILE_SIZE_MB=100
VIDEO_ALLOWED_FORMATS=mp4,mov,avi,mkv,webm
VIDEO_MAX_DURATION_SECONDS=900
VIDEO_MIN_DURATION_SECONDS=30
VIDEO_THUMBNAIL_GENERATION=true
VIDEO_TRANSCRIPTION_AUTO=false
VIDEO_PROCESSING_QUEUE=redis_queue
VIDEO_RECOMMENDATION_ENABLED=true
VIDEO_ANALYTICS_ENABLED=true
VIDEO_AUTO_TAGGING_ENABLED=true

# Payment Gateway Configuration
RAZORPAY_KEY_SECRET=your-razorpay-key-secret
RAZORPAY_WEBHOOK_SECRET=your-razorpay-webhook-secret
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Feature Flags (Legacy)
ENABLE_MOCK_DATA=false
ENABLE_ANALYTICS=true
ENABLE_PAYMENT_PROCESSING=false
ENABLE_CHATBOT=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log
LOG_JSON_FORMAT=false

# Monitoring Configuration
ENABLE_METRICS=true
METRICS_PORT=9090
PROMETHEUS_ENABLED=true

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:8012,http://localhost:3013
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOW_HEADERS=*

# File Upload Configuration
MAX_UPLOAD_SIZE_MB=10
UPLOAD_DIR=./uploads
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Compliance Configuration
IRDAI_COMPLIANCE_ENABLED=true
DPDP_COMPLIANCE_ENABLED=true
AUDIT_LOGGING_ENABLED=true
DATA_ENCRYPTION_ENABLED=true

# Content Management
CONTENT_MAX_FILE_SIZE_MB=100
CONTENT_ALLOWED_VIDEO_TYPES=video/mp4,video/avi,video/mov,video/mkv,video/webm
CONTENT_ALLOWED_DOC_TYPES=application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document
CONTENT_CDN_ENABLED=true
CONTENT_COMPRESSION_ENABLED=true

# Analytics Configuration
ANALYTICS_RETENTION_DAYS=90
ANALYTICS_CACHE_TTL=3600
ANALYTICS_REPORTING_ENABLED=true

# Chatbot Configuration
CHATBOT_ENABLED=true
CHATBOT_MODEL=gpt-3.5-turbo
CHATBOT_TEMPERATURE=0.7
CHATBOT_MAX_TOKENS=300
CHATBOT_LANGUAGE=en
CHATBOT_AUTO_ESCALATION_ENABLED=true
CHATBOT_PROACTIVE_SUGGESTIONS_ENABLED=true

# Callback Management
CALLBACK_SLA_HIGH=15
CALLBACK_SLA_MEDIUM=60
CALLBACK_SLA_LOW=240
CALLBACK_AUTO_ASSIGN=true
CALLBACK_ESCALATION_HOURS=24

# ========================================
# REACT CONFIG PORTAL CONFIGURATION
# ========================================
# Portal Service Configuration
PORTAL_APP_NAME=Agent Configuration Portal
PORTAL_APP_VERSION=1.0.0
PORTAL_DEBUG=true
PORTAL_ENVIRONMENT=development
PORTAL_HOST=0.0.0.0
PORTAL_PORT=3013
PORTAL_JWT_SECRET_KEY=portal-secret-key-change-in-production
PORTAL_JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60
PORTAL_CORS_ORIGINS=http://localhost:3000,http://localhost:3013

# Main API Connection
MAIN_API_URL=http://localhost:8012
MAIN_API_TOKEN=

